{
    "name": "risetechapps/erp-api-code-generate",
    "description": "",
    "keywords": [
        "risetechapps",
        "Rise Tech",
        "code-generate"
    ],
    "homepage": "https://github.com/risetechapps/erp-api-code-generate",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Rise Tech",
            "email": "apps@risetech.combr",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "RiseTech\\CodeGenerate\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RiseTech\\CodeGenerate\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "RiseTech\\CodeGenerate\\CodeGenerateServiceProvider"
            ],
            "aliases": {
                "CodeGenerate": "RiseTech\\CodeGenerate\\CodeGenerateFacade"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
